{
  "name": "bhammyfarms",
  "version": "0.1.0",
  "private": true,
  "scripts": {},
  "dependencies": {
    "debug": "^4.3.2",
    "dotenv": "^8.2.0",
    "qrcode-terminal": "^0.12.0",
    "uuid": "^8.2.0",
    "websocket": "^1.0.34",
    "whatsapp-web.js": "^1.13.2",
    "yarn": "^1.22.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-dangle": "off",
      "max-len": [
        2,
        {
          "code": 120,
          "ignoreUrls": true
        }
      ],
      "no-underscore-dangle": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "devDependencies": {
    "natural": "^5.1.1",
    "sequelize-cli": "^5.4.0"
  }
}
