{
  "type": "module",
  "name": "bhammyfarms",
  "version": "0.1.0",
  "private": true,
  "scripts": {},
  "dependencies": {
    "body-parser": "^1.19.0",
    "chromium": "^3.0.3",
    "debug": "^4.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "formidable": "^1.2.2",
    "html-pdf": "^3.0.1",
    "html-pdf-node": "^1.0.7",
    "moment": "^2.29.0",
    "mysql2": "^2.1.0",
    "node-html-parser": "^5.1.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.11",
    "openai": "^3.2.1",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "puppeteer": "^10.4.0",
    "qrcode-terminal": "^0.12.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.2.0",
    "websocket": "^1.0.34",
    "yarn": "^1.22.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-dangle": "off",
      "max-len": [
        2,
        {
          "code": 120,
          "ignoreUrls": true
        }
      ],
      "no-underscore-dangle": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "devDependencies": {
    "natural": "^5.1.1",
    "sequelize-cli": "^5.4.0"
  }
}
