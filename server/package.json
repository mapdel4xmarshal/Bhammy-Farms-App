{
  "name": "bhammyfarms",
  "version": "0.1.0",
  "private": true,
  "scripts": {},
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "formidable": "^1.2.2",
    "moment": "^2.29.0",
    "mysql2": "^2.1.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "sequelize": "^6.3.5",
    "uuid": "^8.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-dangle": "off",
      "max-len": [
        2,
        {
          "code": 120,
          "ignoreUrls": true
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
